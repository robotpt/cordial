<?xml version="1.0"?>
<package format="2">
  <name>cordial_manager</name>
  <version>0.0.1</version>
  <description >
    An interface to Amazon's Polly service for text-to-speech, visemes (which are
    used for animation), and figuring out when to do gestures during speech.
  </description>
  <maintainer email="audrow.nash@gmail.com">Audrow Nash</maintainer>
  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>python-boto3</build_depend>
  <build_depend>python-botocore</build_depend>
  <build_depend>python-soundfile</build_depend>

  <build_export_depend>actionlib</build_export_depend>

  <exec_depend>actionlib</exec_depend>
</package>
